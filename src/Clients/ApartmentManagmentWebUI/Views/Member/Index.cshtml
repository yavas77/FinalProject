@model UserListModel
@{
	ViewData["Title"] = "Index";
	Layout = "_MemberLayout";
}


<!-- Content Header (Page header) -->
<div class="content-header">
	<div class="container-fluid">
		<div class="row mb-2">
			<div class="col-sm-12">
				<h1 class="m-0">Bilgilerim listesi</h1>
			</div><!-- /.col -->
		</div><!-- /.row -->
	</div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
<!-- Main content -->
<section class="content">
	<div class="container-fluid">
		<!-- /.row -->
		<div class="row">
			<div class="col-12">
				<div class="card">
					<div class="card-header">
						<h3 class="card-title"></h3>

						<div class="card-tools">
						
						</div>
					</div>
					<!-- /.card-header -->

					<div class="card-body table-responsive p-0">
						<table class="table table-hover text-nowrap">
							<thead>
								<tr>
									<th>Ad Soyad</th>
									<th>Telefon</th>
									<th>Email</th>
									<th>TC</th>
									<th>Araç Plakası</th>
									<th>Dairesi</th>
									<th>Bakiyesi</th>
								</tr>
							</thead>
							<tbody>

								<tr>
									<td>@Model.FirstName @Model.LastName</td>
									<td>@Model.PhoneNumber</td>
									<td>@Model.Email</td>
									<td>@Model.TC</td>
									<td>@Model.Plate</td>
									<td>@Model.Apartment</td>
									<td>@Model.Balance</td>

								</tr>


							</tbody>
						</table>
					</div>
					<!-- /.card-body -->

				</div>
				<!-- /.card -->
			</div>
		</div>
		<!-- /.row -->
	</div><!-- /.container-fluid -->
</section>